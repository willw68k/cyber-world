pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
-- cyber world

molly = {}
molly.x = 0
molly.y = 0
molly.sprt = 1
molly.tmr = 1
molly.flp = false

apartm = {}
apartm.x0 = 10
apartm.y0 = 10
apartm.x1 = 100
apartm.y1 = 46

current_lvl = {}
current_lvl.number = 0

-- character move function
function move_molly(bl,br)
	molly.tmr = molly.tmr+1
	if molly.tmr>=60 then
		molly.sprt = 16
	end
	if btn(1) then
		if molly.x < br then
			molly.flp = false
		 molly.x=molly.x+0.5
		 molly.sprt=molly.sprt+0.5
		 molly.tmr = 0
		 if molly.sprt>=5 then
	 		molly.sprt = 1
		 end
		end
	elseif btn(0) then
		if molly.x > bl then
			molly.flp = true
			molly.x=molly.x-0.5
		 molly.sprt=molly.sprt+0.5
			molly.tmr = 0
			if molly.sprt>=5 then
	 		molly.sprt = 1
		 end
		end
	end
end

function lvl_change(ln)
	current_lvl.number = ln
	if current_lvl.number==1 then 
		molly.x = ((apartm.x0 + apartm.x1) /2) - 8
		molly.y = apartm.y1 - 8
	end
end

function draw_lvl()
	if current_lvl.number==1 then 
		rectfill(apartm.x0,apartm.y0,apartm.x1,apartm.y1, 5)
		rectfill(apartm.x0+10, apartm.y0+27, apartm.x1-40, apartm.y1-32, 8)
		--line(apartm.x0+10, apartm.y0+14, apartm.x0+50, apartm.y0+14, 12)
		--line(apartm.x0+10, apartm.y0+13, apartm.x0+50, apartm.y0+13, 9)
		--rectfill(apartm.x0+10, apartm.y0+12, apartm.x1-30, apartm.y1-30, 1)
		--line(apartm.x0+10, apartm.y0+12, apartm.x0+50, apartm.y0+12, 12)
		
		d = 1
		for c=0,4 do
			rectfill(apartm.x0+10, (apartm.y0+28)-d, apartm.x1-40, (apartm.y1-8)-d, 9)
			d = c * 3.5
		end
		spr(15,apartm.x0+44,apartm.y0+13,1,2,false)
		spr(13,apartm.x0+28,apartm.y0+13,2,2,false)
		spr(29,apartm.x0+10,apartm.y0+21,2,1,true)
	end
end

function _init() 
	cls()
	lvl_change(1)
end

function _update()
	if current_lvl.number==1 then
		bnd_left = apartm.x0
		bnd_right = apartm.x1 + 8
	end
	move_molly(bnd_left, bnd_right)
end

function _draw()
	cls()
	print("cyber world", 0, 0, 7)
	draw_lvl()
	spr(molly.sprt,molly.x,molly.y,1,1,molly.flp)
end
