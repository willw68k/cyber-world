pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
-- cyber world

molly = {}
molly.x = 0
molly.y = 0
molly.sprt = 1
molly.tmr = 1
molly.flp = false

apartm = {}
apartm.x0 = 10
apartm.y0 = 10
apartm.x1 = 100
apartm.y1 = 46

current_lvl = {}
current_lvl.number = 0

-- character move function
function move_molly(bl,br)
	molly.tmr = molly.tmr+1
	if molly.tmr>=60 then
		molly.sprt = 32
	end
	if btn(1) then
		if molly.x < br then
			molly.flp = false
			molly.x=molly.x+0.6
			molly.sprt=molly.sprt+0.4
			molly.tmr = 0
		 if molly.sprt>=5 then
	 		molly.sprt = 1
		 end
		end
	elseif btn(0) then
		if molly.x > bl then
			molly.flp = true
			molly.x=molly.x-0.6
			molly.sprt=molly.sprt+0.4
			molly.tmr = 0
			if molly.sprt>=5 then
	 			molly.sprt = 1
			end
		end
	end
end

function lvl_change(ln)
	current_lvl.number = ln
	if current_lvl.number==1 then 
		molly.x = ((apartm.x0 + apartm.x1) /2) - 8
		molly.y = apartm.y1 - 15
	end
end

function draw_lvl()
	if current_lvl.number==1 then 
		rectfill(apartm.x0,apartm.y0,apartm.x1,apartm.y1, 6)
		rectfill(apartm.x0,apartm.y0+15,apartm.x1,apartm.y1-37, 6)
--[[
		for iz=0, 16 do
			for iw=0, 6 do
				line(10, 25+ih, 100, 25+ih, 6)
			end
			for i=0,42,2 do
				pset(apartm.p+is, 34,6)
				pset(apartm.p+iso, 34,6)
			end
			ih=ih*1.2
			is=is+5.5
			iso=iso+4.5
			
		end
--]]
		--rectfill(apartm.x0,apartm.y0,apartm.x1,apartm.y1, 5)
		rectfill(20, 37, 60, 14, 8)
		rect(19, 39, 61, 14, 5)
		--line(19, 14, 61, 14, 5)
		--line(19, 14, 19, 38, 5)
		
		--line(apartm.x0+10, apartm.y0+13, apartm.x0+50, apartm.y0+13, 9)
		--rectfill(apartm.x0+10, apartm.y0+12, apartm.x1-30, apartm.y1-30, 1)
		--line(apartm.x0+10, apartm.y0+12, apartm.x0+50, apartm.y0+12, 12)
		
		d = 1.5
		for c=0,5 do
			rectfill(apartm.x0+10, (apartm.y0+28)-d, apartm.x1-40, (apartm.y1-8)-d, 9)
			d = c *3.5
			
		end
		spr(15,apartm.x0+44,apartm.y0+13,1,2,false)
		spr(13,apartm.x0+28,apartm.y0+13,2,2,false)
		spr(29,apartm.x0+10,apartm.y0+21,2,1,true)
		--hole
		spr(11,77,30,1,1,false)
		--table
		spr(12,64,39,1,1,false)
		line(72, 39, 75, 39, 4)
		line(72, 40, 75, 40, 4)
		spr(12,76,39,1,1,true)
		--poster
		rectfill(67, 18, 77, 25, 13)
		spr(28,68,18,1,1,false)
		
		--door
		rectfill(86, 46, 99, 24, 4)
		
		dn = 1
		for dn1=0, 1 do
			for iw=0, 2 do
				pset(87+dn, 37, 9)
			end
			for iw=0, 2 do
				pset(87+dn, 38, 5)
			end
			
			dn=dn+1
			
		end
	end
end

function _init() 
	cls()
	lvl_change(1)
end

function _update()
	if current_lvl.number==1 then
		bnd_left = apartm.x0
		bnd_right = apartm.x1 + 8
	end
	move_molly(bnd_left, bnd_right)
end

function _draw()
	cls()
	print("cyber world", 0, 0, 7)
	draw_lvl()
	spr(molly.sprt,molly.x,molly.y,1,2,molly.flp)
end
